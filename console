#!/usr/bin/env php

<?php

require __DIR__ . '/vendor/autoload.php';

use Gammabeam82\Benchmark\Command\BenchmarkCommand;
use Symfony\Component\Console\Application;

set_time_limit(600);

$app = new Application();
$app->add(new BenchmarkCommand());

/** @noinspection PhpUnhandledExceptionInspection */
$app->run();
